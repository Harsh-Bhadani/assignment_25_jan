//4.	Write a c++ program that takes a double input from the user and then converts it to an integer using a type cast, rounding down to the nearest integer if necessary.
//---------------------------------------------------------------


#include<bits/stdc++.h>
#include <cmath>
using namespace std;

int main()
{
    double d;
    cin>>d;

    double b=round(d);
    cout<<(int)b<<endl;

}
